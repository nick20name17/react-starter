{
  "tabWidth": 2,
  "useTabs": false,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "arrowParens": "always",
  "semi": false,
  "trailingComma": "none",
  "endOfLine": "crlf",
  "printWidth": 110,
  "bracketSameLine": false,
  "singleAttributePerLine": true,
  "bracketSpacing": true,
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "plugins": ["@trivago/prettier-plugin-sort-imports", "prettier-plugin-tailwindcss"],
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^api/(.*)$",
    "^assets/(.*)$",
    "^components/(.*)$",
    "^constants/(.*)$",
    "^config/(.*)$",
    "^context/(.*)$",
    "^hooks/(.*)$",
    "^lib/(.*)$",
    "^pages/(.*)$",
    "^providers/(.*)$",
    "^routes/(.*)$",
    "^store/(.*)$",
    "^types/(.*)$",
    "^utils/(.*)$",
    "^../(.*)",
    "^./(.*)",
    "(.css)$",
    "(.scss)$"
  ]
}
